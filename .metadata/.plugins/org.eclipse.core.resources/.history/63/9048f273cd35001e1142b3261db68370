package interviewPrograms2;

public class Reversearray {
	public static void main(String[] args) {
		int [] a = {2,3,1,3,4};
		int [] b = {};
		for(int i=a.length-1;i>=0;i--) {
			System.out.println(a[i]);
			for(int j =1;j<a.length;j++) {
				System.out.println(j);
				b[j] = a[i];
			}
			
		}
		System.out.println(b);
	}
}
