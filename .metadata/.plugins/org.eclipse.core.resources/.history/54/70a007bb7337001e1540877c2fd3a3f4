package special;

public class Naveen {
	public static void main(String[] args) {
		String a = "kathiravan kaveen";
		String [] b = a.split(" ");
		for(int i=0;i<b.length;i++) {
			for(int j=0;j<b.length;j++) {
				if(b[j].charAt(0) == b[j+1].charAt(0)) {
					if(b[j].charAt(b[i].length()-1) == b[j+1].charAt(b[j+1].length()-1)) {
						System.out.println(b[j]+" "+b[j+1]+" ful fills the given condition");
					}
				}
			}
		}
	}
}
