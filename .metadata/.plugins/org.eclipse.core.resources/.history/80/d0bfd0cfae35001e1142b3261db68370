package interviewPrograms;

import java.util.Arrays;

public class Two {
	public static void main(String[] args) {
		String a = "2-1+3";
		String[] b = a.split("[-]");
		System.out.println(Arrays.toString(b));
		for(String x : b) {
			String[] c = x.split("[+]");
			System.out.println(Arrays.toString(c));
		}
		
	}
}
